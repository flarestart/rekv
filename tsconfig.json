{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "declaration": true,
    "skipLibCheck": true,
    "jsx": "react",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "removeComments": true,
    "outDir": "dist",
    "noEmit": false
  },
  "include": ["src/*.ts"]
}
